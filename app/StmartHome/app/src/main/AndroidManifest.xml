<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eden.stmarthome">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".activities.LoginActivity" android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MainActivity" android:screenOrientation="landscape"></activity>
        <activity android:name=".activities.RegisterActivity" android:screenOrientation="landscape"></activity>
        <activity android:name=".activities.ChangePassActivity" android:screenOrientation="landscape"></activity>
        <activity android:name=".activities.SimpleActivity" android:screenOrientation="landscape"></activity>
        <activity android:name=".activities.SetPassActivity" android:screenOrientation="landscape"></activity>
        <activity android:name=".activities.DeviceActivity" android:screenOrientation="landscape"></activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>